{
  "version": 2,
  "builds": [
    { "src": "api/price.js", "use": "@vercel/node" },
    { "src": "android/app/src/main/assets/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/price", "dest": "/api/price.js" },
    { "src": "/(.*)", "dest": "/android/app/src/main/assets/index.html" }
  ]
}

